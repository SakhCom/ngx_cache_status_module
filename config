ngx_addon_name=ngx_cache_status_module

if test -n "$ngx_module_link"; then
  ngx_module_type=HTTP_FILTER
  ngx_module_name=ngx_cache_status_module
  ngx_module_srcs="$ngx_addon_dir/ngx_cache_status_module.c"
  . auto/module
else
	HTTP_FILTER_MODULES="$HTTP_FILTER_MODULES ngx_cache_status_module"
	NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_cache_status_module.c"
fi